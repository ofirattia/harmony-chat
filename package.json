{
  "name": "harmony-boilerplate",
  "version": "3.0.1",
  "description": "Harmony Boilerplate is based on Multi Model Layer, MongoDB, Express, React and NodeJS",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "test": "mocha --require @babel/register --require ./client/test/test_helper.js --recursive ./client/test/components ",
    "test:watch": "cross-env NODE_ENV=development  npm run test --  --watch ",
    "test:report": "cross-env NODE_ENV=development mocha --require @babel/register --require ./client/test/test_helper.js --recursive ./client/test/components --reporter mochawesome --reporter-options reportDir=./client/test/reports",
    "clear": "rimraf ./client/dist",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "startserver:prod": "cross-env NODE_ENV=production && npm-run-all --serial build:server start:server",
    "startserver:dev": "cross-env NODE_ENV=development nodemon -w ./server --exec \"babel-node --inspect ./server/server.js \"",
    "apidocs": "cd server && apidoc -i src/api -o docs",
    "build:prod": "cross-env NODE_ENV=production  webpack --watch --config ./webpack.config.prod.js",
    "build:prod:client": "npm run sass && cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "start:prod": "npm run clear && npm-run-all --parallel sass build:prod startserver:prod test:watch  sass:watch",
    "build:dev": "cross-env NODE_ENV=development  webpack --progress --watch --config ./webpack.config.dev.js",
    "start:dev": "npm run clear && npm-run-all --parallel sass  startserver:dev build:dev sass:watch",
    "start": "npm run start:dev"
  },
  "author": "",
